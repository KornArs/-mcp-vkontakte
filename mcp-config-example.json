{
  "mcpServers": {
    "vkontakte-stdio": {
      "command": "node",
      "args": ["dist/cursor-mcp-server.js"],
      "description": "VKontakte MCP server via STDIO (local)",
      "disabled": false,
      "env": {
        "VK_ACCESS_TOKEN": "your_vk_access_token_here",
        "VK_API_VERSION": "5.199"
      }
    },
    "vkontakte-http": {
      "url": "https://mcp-vkontakte-production.up.railway.app/mcp",
      "description": "VKontakte MCP server via HTTP (Railway)",
      "disabled": false,
      "headers": {
        "x-vk-access-token": "your_vk_access_token_here"
      }
    }
  }
}